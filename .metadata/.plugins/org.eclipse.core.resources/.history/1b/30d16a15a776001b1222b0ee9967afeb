import javax.swing.JOptionPane;

public class Lacos {
	public static void main(String[] args) {
		
		// tabuada
		int num=7, i=0;
		String mensagem = "";
		
		while (num < 1)  {
			num = Integer.valueOf(JOptionPane.showInputDialog("Entre com um número"));	
		} 
		
		/*
		while (i < 11) {
			mensagem +=  num +" * "+ i + " = "+(num * i)+"\n";
			i++;
		}
		JOptionPane.showMessageDialog(null, mensagem);
		*/
		do {
			mensagem +=  num +" * "+ i + " = "+(num * i)+"\n";
			i++;
		} while(i < 11);
		JOptionPane.showMessageDialog(null, mensagem);
	}
	
}
