import javax.swing.JOptionPane;

public class Lacos {
	public static void main(String[] args) {
		
		// tabuada
		int num, i=0;
		String mensagem = "";
		num = Integer.valueOf(JOptionPane.showInputDialog("Entre com um número"));	
		
		while (true) {
			if (i < 11)
				break;
			mensagem +=  num +" * "+ i + " = "+(num * i)+"\n";
			i++;
		}
		JOptionPane.showMessageDialog(null, mensagem);
		
	}
	
}
